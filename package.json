{
  "name": "bedoc",
  "version": "1.0.0",
  "description": "Pluggable documentation engine for any language and format",
  "main": "./src/core/Core.js",
  "bin": {
    "bedoc": "./src/cli.js"
  },
  "exports": {
    "./*": "./src/*"
  },
  "imports": {
    "#core": "./src/core/index.js",
    "#util": "./src/core/util/index.js",
    "#action": "./src/core/action/index.js"
  },
  "type": "module",
  "scripts": {
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint . --fix"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "error-stack-parser": "^2.1.4",
    "eslint-plugin-jsdoc": "^50.6.2",
    "globby": "^14.0.2",
    "micromatch": "^4.0.8",
    "node-fetch": "^3.3.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.13.0",
    "eslint": "^9.18.0"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "bedoc.generate",
        "title": "Generate Documentation"
      }
    ]
  },
  "keywords": [
    "documentation",
    "plugin",
    "parser",
    "printer"
  ],
  "author": "gesslar",
  "license": "Unlicense"
}
