{
  "name": "@gesslar/bedoc",
  "description": "Pluggable documentation engine for any language and format",
  "author": {
    "name": "gesslar",
    "url": "https://gesslar.dev"
  },
  "version": "1.11.0",
  "license": "Unlicense",
  "homepage": "https://github.com/gesslar/lpc-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gesslar/BeDoc.git"
  },
  "keywords": [
    "documentation",
    "plugin",
    "parser",
    "printer",
    "generator",
    "language",
    "format",
    "hooks"
  ],
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=22"
  },
  "bin": {
    "bedoc": "src/cli.js"
  },
  "exports": {
    ".": {
      "import": "./src/BeDoc.js",
      "types": "./dist/types/BeDoc.d.ts"
    },
    "./BeDoc": {
      "import": "./src/BeDoc.js",
      "types": "./dist/types/BeDoc.d.ts"
    },
    "./*": {
      "import": "./src/*",
      "types": "./dist/types/*"
    }
  },
  "type": "module",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "types": "tsc -p tsconfig.types.json",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "update": "pnpm up --latest --recursive",
    "submit": "pnpm publish --access public --//registry.npmjs.org/:_authToken=\"${NPM_ACCESS_TOKEN}\"",
    "pr": "gt submit -p --ai",
    "patch": "pnpm version patch",
    "minor": "pnpm version minor",
    "major": "pnpm version major"
  },
  "dependencies": {
    "@gesslar/actioneer": "link:../Actioneer",
    "@gesslar/colours": "^0.8.0",
    "@gesslar/negotiator": "link:../Negotiator",
    "@gesslar/toolkit": "^3.35.1",
    "bedoc-lpc-parser": "link:examples/node_modules_test/bedoc-lpc-parser",
    "bedoc-lua-parser": "link:examples/node_modules_test/bedoc-lua-parser",
    "bedoc-markdown-printer": "link:examples/node_modules_test/bedoc-markdown-printer",
    "bedoc-wikitext-printer": "link:examples/node_modules_test/bedoc-wikitext-printer",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "error-stack-parser": "^2.1.4",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@gesslar/uglier": "^1.4.1",
    "axios": "^1.13.5",
    "chokidar": "^5.0.0",
    "eslint": "^10.0.0",
    "form-data": "^4.0.5",
    "typescript": "^5.9.3"
  }
}
