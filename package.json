{
  "name": "@gesslar/bedoc",
  "description": "Pluggable documentation engine for any language and format",
  "author": {
    "name": "gesslar",
    "url": "https://gesslar.dev"
  },
  "version": "1.11.0",
  "license": "Unlicense",
  "homepage": "https://github.com/gesslar/BeDoc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gesslar/BeDoc.git"
  },
  "keywords": [
    "documentation",
    "plugin",
    "parser",
    "formatter",
    "generator",
    "language",
    "format",
    "hooks"
  ],
  "engines": {
    "node": ">=22"
  },
  "bin": {
    "bedoc": "src/cli.js"
  },
  "exports": {
    ".": {
      "import": "./src/BeDoc.js",
      "types": "./dist/types/BeDoc.d.ts"
    },
    "./BeDoc": {
      "import": "./src/BeDoc.js",
      "types": "./dist/types/BeDoc.d.ts"
    },
    "./*": {
      "import": "./src/*",
      "types": "./dist/types/*"
    }
  },
  "type": "module",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "types": "tsc -p tsconfig.types.json",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "update": "npx npm-check-updates -u && npm install",
    "submit": "npm publish --access public --//registry.npmjs.org/:_authToken=\"${NPM_ACCESS_TOKEN}\"",
    "pr": "gt submit -p --ai",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "dependencies": {
    "@gesslar/actioneer": "^2.3.1",
    "@gesslar/colours": "^0.8.0",
    "@gesslar/negotiator": "^0.4.0",
    "@gesslar/toolkit": "^3.37.0",
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@gesslar/uglier": "^1.4.1",
    "bedoc-lpc-parser": "file:examples/node_modules_test/bedoc-lpc-parser",
    "bedoc-lua-parser": "file:examples/node_modules_test/bedoc-lua-parser",
    "bedoc-markdown-formatter": "file:examples/node_modules_test/bedoc-markdown-formatter",
    "bedoc-wikitext-formatter": "file:examples/node_modules_test/bedoc-wikitext-formatter",
    "dotenv": "^17.3.1",
    "eslint": "^10.0.0",
    "typescript": "^5.9.3"
  }
}
